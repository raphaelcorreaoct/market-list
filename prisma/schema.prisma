
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}


model List {
  id        String @id @default(uuid())
  name      String 
  products  Products[] 
  date      DateTime @default(now())
}


model Products {
  id        String @id @default(uuid())
  name      String
  price     Float
  quantity  Int
  listId    String?
  list      List?   @relation(fields: [listId], references: [id])
}